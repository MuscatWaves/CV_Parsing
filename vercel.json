{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://cvapi.muscatwave.com/api/:path*"
      },
      {
        "source": "/api/:path*/",
        "destination": "https://cvapi.muscatwave.com/api/:path*/"
      },
      {
        "source": "/files/:path*",
        "destination": "https://cvparse.fra1.cdn.digitaloceanspaces.com/files/:path*"
      },
      {
        "source": "/files/:path*/",
        "destination": "https://cvparse.fra1.cdn.digitaloceanspaces.com/files/:path*/"
      }
    ]
  }